        print("* ", end = " ")
    print(" ") 

n=6
for i in range(0,n):
    for j in range(i):
        print("* ", end = " ")
    print(" ")     

#Reverse loop

n=6
for i in range(0,n):
    for j in range(i):
        print("*", end=" ")
    print(" ")    
for i in range(n, 0, -1):
    for j in range(i):
        print("*", end=" ")
    print(" ") 

n=4
for i in range(0,n):
    for j in range(i):
        print("*", end=" ")
    print(" ")
for i in range(n, 0, -1):
    for j in range(i):
        print("*", end=" ")
    print(" ") 

n=5
for i in range(0,n):
    for j in range(i):
        print("2", end=" ")
    print(" ")     
for i in range(n,0,-1):
    for j in range(i):
        print("1", end=" ")
    print(" ")   

n=5
print(" * "*n)     

n=5
for i in range(n):
    print(" * ")

n=5
